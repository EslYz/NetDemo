<resources>

    <attr name="headerBg" format="reference" />

    <style name="HeaderTheme" parent="Theme.Fauna">
        <item name="headerBg">@color/fauna_header_bg_color</item>
    </style>

    <style name="SalesTheme" parent="Theme.Fauna">
        <item name="headerBg">@color/choose_customer_color</item>
    </style>

    <style name="header">
        <item name="android:background">?attr/headerBg</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">45dp</item>
    </style>

    <style name="headerTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:textSize">45dp</item>
        <item name="android:textColor">@color/black_1</item>
    </style>

    <style name="headerRightText">
        <item name="android:textColor">@color/black_1</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingLeft">15dp</item>
        <!--<item name="android:background">@drawable/header_button_bg</item>-->
    </style>

    <style name="headerRightTextStyle">
        <item name="android:textColor">@color/black_1</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">16sp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingLeft">9dp</item>
        <!--<item name="android:background">@drawable/header_button_bg</item>-->
    </style>

    <style name="headerRightWithMsgText">
        <item name="android:textColor">@color/black_1</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">16sp</item>
        <!--<item name="android:layout_alignParentRight">true</item>-->
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingLeft">15dp</item>
        <!--<item name="android:background">@drawable/header_button_bg</item>-->
    </style>

    <style name="headerLeftText">
        <item name="android:textColor">@color/black_1</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingLeft">15dp</item>
        <!--<item name="android:background">@drawable/header_button_bg</item>-->
